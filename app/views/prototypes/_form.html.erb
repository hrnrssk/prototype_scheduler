<%= form_with(model: @prototype, local: true ,url: prototype_choose_new_or_edit ) do |f| %>
  <% if @prototype.errors.any? %>
    <div id="error_explanation">
      <h2><%= @prototype.errors.count %>件のエラーがあります。</h2>
      <ul>
      <% @prototype.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="container">
    <div class="row mt-4">
      <div class="col-2 offset-4">
        <%= f.label :注文番号 %>
      </div>
      <div class="col-2">
        <%= f.text_field :order_number %>
      </div>
      <div class="col-2">
        *整数8文字
      </div>
    </div>
    <div class="row mt-3">
      <div class="col-2 offset-4">
        <%= f.label :得意先 %>
      </div>
      <div class="col-2">
        <%= f.text_field :customer %>
      </div>
    </div>
    <div class="row mt-3">
      <div class="col-2 offset-4">
        <%= f.label :品名 %>
      </div>
      <div class="col-2">
        <%= f.text_field :name %>
      </div>
    </div>
    <div class="row mt-3">
      <div class="col-2 offset-4">
        <%= f.label :納期 %>
      </div>
      <div class="col-2">
        <%= f.text_field :delivery_date %>
      </div>
    </div>
  </div>
  <div class="row mt-3">
    <%= f.submit "情報更新", class: "btn btn-primary" %>
  </div>
<% end %>